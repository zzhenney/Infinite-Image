  <!-- Navigation -->
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">
    <div class="container">
      <%= link_to "#{image_tag("infiniteLogo.png", class: "navbar-brand img-fluid")}".html_safe, "/" %>
      <a class="navbar-brand" href="/">Infinite Image</a>
      <div class="collapse navbar-collapse" id="navbarResponsive">
        <ul class="navbar-nav ml-auto">

          <li class="nav-item active">
            <% if signed_in? %>
              <% if current_user != nil && !current_user.is_admin? %>
              <li class="nav-item">
                <%= link_to 'Upload', new_image_path, class: "nav-link" %>
              </li>
            <% end %>
          <% end %>
          </li>

          <li class="nav-item">
            <% if current_user == nil || signed_in? && !current_user.is_admin? %>
              <%= link_to "About", pages_path, class: "nav-link" %>
            <% end %>
          </li>

          <!--        this is helpful for admin     Paul Ancajima-->
          <li class="nav-item">
            <% if current_user.present? %>
              <% if current_user.is_admin? %>
                <%= link_to "Images", images_path, class: "nav-link" %>
              <% end %>
            <% end %>

          </li>

          <li class="nav-item">
            <% if !signed_in? %>
              <%= link_to "Register", sign_up_path, class: "nav-link" %>
            <% end %>
          </li>

          <!-- Login -->
          <li class="nav-item">
            <% if signed_in? %>
              <%= link_to "Sign Out", sign_out_path, method: :delete, class: "nav-link" %>
              </li>
            <% else %>
              <li class="nav-item">
                <%= link_to "Login", sign_in_path, method: :get, class: "nav-link" %>
            <% end %>
            </li>
            <!-- End Login -->

            </ul>
      </div>
    </div>
  </nav>

